<li class="span4">
    <div class="thumbnail <%= tour.topic_list.join(' ')%> <%= tour.time_list.join(' ') %>" style="background-image: url(<%= tour.image.url %>); height: 250px;">
      <h2><%= tour.title %></h2>
      <a href="<%= tour_path(tour) %>" class="btn btn-success btn-large">$<%= tour.price %> for <%= pluralize(tour.hours, 'hour') %></a>
    </div>
</li>
